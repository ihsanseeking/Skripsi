#2===============================================================================
	SELECT CONCAT_WS(
		' ',
		first_name,
		last_name
	) AS "Nama Pelanggan",
	DATEDIFF(
		return_date,
		rental_date
	) AS "Lama Sewa (hari)"

	WHERE DATEDIFF(
		return_date,
		rental_date
	) > 3
	GROUP BY customer_id
#3===============================================================================
	
	SELECT 
		DISTINCT title AS "Judul Film"
	FROM film 
		JOIN inventory USING (film_id)
	WHERE store_id=2 
		AND store_id<>1 
		
	[762 baris]
		
	SELECT 
		DISTINCT film_id,
		title AS "Judul Film"
	FROM film 
		JOIN inventory USING (film_id)
	WHERE store_id=2 
		AND film_id NOT IN 	
	(
		SELECT
			film_id 
		FROM inventory 
		WHERE store_id=1
	) 
	

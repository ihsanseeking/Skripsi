#1===============================================================================

	SELECT 
		CONCAT_WS(
			' ',
			first_name,
			last_name
		) AS NamaLengkap,
		title AS Film
	FROM actor 
		JOIN film_actor USING (actor_id) 
		JOIN film USING (film_id)
	ORDER BY NamaLengkap ASC 
	
	ATAU
	
	SELECT 
		CONCAT_WS(
			' ',
			first_name,
			last_name
		) AS NamaLengkap, 
		GROUP_CONCAT(title) AS Film
	FROM actor 
		JOIN film_actor USING (actor_id) 
		JOIN film USING (film_id)
	GROUP BY NamaLengkap
	ORDER BY NamaLengkap ASC 
	
